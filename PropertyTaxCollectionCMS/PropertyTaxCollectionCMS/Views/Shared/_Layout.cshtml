        .btn-custom {
            background-image: linear-gradient(to right, #faed47 0%, #fdab01 51%, #faed47 100%);
            background-size: 200% auto;
            -webkit-transition: 0.5s ease-in-out;
            transition: 0.5s ease-in-out;
            color: #fff;
        }

            .btn-custom:active {
                background-image: linear-gradient(to right, #faed47 0%, #fdab01 51%, #faed47 100%);
            }

            .btn-custom:focus {
                outline: 0;
                box-shadow: 0 0 0 3px rgba(253,194,16,0.5);
            }

            .btn-custom:hover {
                background-position: right center;
            }

        .btn-file input[type=file] {
            position: absolute;
            top: 0;
            right: 0;
            min-width: 100%;
            min-height: 100%;
            font-size: 100px;
            overflow: hidden;
            text-align: center;
            filter: alpha(opacity=0);
